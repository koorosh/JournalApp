{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": {
      "options": "never"
    },
    "ordered-imports": false,
    "quotemark": {
      "options": "single"
    },
    "object-literal-sort-keys": false,
    "variable-name": false,
    "import-name": false,
    "interface-name": {
      "options": "never-prefix"
    },
    "align": false,
    "no-duplicate-imports": false,
    "jsx-boolean-value": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false
  },
  "rulesDirectory": []
}
